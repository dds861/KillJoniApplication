<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.chatserver.dd.chat">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_rifle"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_rifle"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.chatserver.dd.chat.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.chatserver.dd.chat.Menu1.ActivityMenu1" android:parentActivityName=".MainActivity" />
        <activity android:name="com.chatserver.dd.chat.Menu2.ActivityMenu2" android:parentActivityName=".MainActivity"/>
    </application>

</manifest>